<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd"
         version="3.1">

    <display-name>com.genestimate.webapp</display-name>
    <enterprise-beans>
        <session>
            <ejb-name>ProductEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.ProductBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>BindingTypeEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.BindingTypeBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>ClientEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.ClientBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>ComponentEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.ComponentBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>ComponentPropertiesEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.ComponentPropertiesBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>EstimateEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.EstimateBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>MachineEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.MachineBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>PrintingTypeEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.PrintingTypeBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>PropertiesEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.PropertiesBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>RawMaterialEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.RawMaterialBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>DimensionsEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.DimensionsBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>LoginEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.LoginBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>UserEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.UserBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <ejb-name>AuthEJB</ejb-name>
            <ejb-class>com.genestimate.webapp.ejbs.AuthBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>

</ejb-jar>
